@article{Grassberger2016,
abstract = {We present simulations of the 1-dimensional Oslo rice pile model in which the critical height at each site is randomly reset after each toppling. We use the fact that the stationary state of this sandpile model is hyperuniform to reach system of sizes {\$}{\textgreater} 10{\^{}}7{\$}. Most previous simulations were seriously flawed by important finite size corrections. We find that all critical exponents have values consistent with simple rationals: {\$}\backslashnu=4/3{\$} for the correlation length exponent, {\$}D =9/4{\$} for the fractal dimension of avalanche clusters, and {\$}z=10/7 {\$} for the dynamical exponent. In addition we relate the hyperuniformity exponent to the correlation length exponent {\$}\backslashnu{\$}. Finally we discuss the relationship with the quenched Edwards-Wilkinson (qEW) model, where we find in particular that the local roughness exponent is {\$}\backslashalpha{\_}{\{}\backslashrm loc{\}} = 1{\$}.},
archivePrefix = {arXiv},
arxivId = {1606.02553},
author = {Grassberger, Peter and Dhar, Deepak and Mohanty, P. K.},
doi = {10.1103/PhysRevE.94.042314},
eprint = {1606.02553},
file = {:C$\backslash$:/Users/Lingyi/Google Drive/Mendeley/pdfs/Grassberger, Dhar, Mohanty - 2016 - Oslo model, hyperuniformity, and the quenched Edwards-Wilkinson model.pdf:pdf},
issn = {15502376},
journal = {Physical Review E - Statistical, Nonlinear, and Soft Matter Physics},
number = {4},
title = {{Oslo model, hyperuniformity, and the quenched Edwards-Wilkinson model}},
volume = {94},
year = {2016}
}
@article{lagache1998,
abstract = {Using DIRBE and FIRAS maps at high latitude ({\$}|b|{\textgreater}10{\^{}}{\{}\backslashcirc{\}}{\$}) we derive the spatial distribution of the dust temperature associated with the diffuse cirrus and the dense molecular clouds. For a {\$}\nu{\^{}}2{\$} emissivity law, we find that the equilibrium dust temperature of the cirrus is about 17.5 K with only small variations over the high latitude sky. Comparison of the far Infrared DIRBE maps shows the presence of a colder emission component with a temperature around 15 K, assuming a {\$}\nu{\^{}}2{\$} emissivity law. The lowest values of the temperature found in the cold regions ({\$}{\~{}}13 K{\$}) are compatible with the results recently obtained for dense cores in star forming regions by the balloon-borne experiment SPM-PRONAOS (Ristorcelli et al., 1996, 1998, Serra et al., 1997). This cold component is in particular present in the direction of known molecular complexes with low star forming activity such as Taurus. The association between the cold component and molecular clouds is further demonstrated by the fact that all sky pixels with significant cold emission have an excess IR emission with respect to the high latitude IR/HI correlation. We have deduced a threshold value of the column density, {\$}N{\_}{\{}HI{\}} = 2.5 10{\^{}}{\{}20{\}} H/cm{\^{}}2{\$}, below which cold dust is not detected within the FIRAS beam of {\$}{\~{}}7\backslashdegr{\$}. We have re-examined the problem of the existence of a very cold dust component ({\$}T {\~{}}7 K{\$}) by combining DIRBE maps of the cold emission with FIRAS spectra, corrected for the isotropic component found in Puget et al. (1996). The warm and cold component deduced from the analysis of DIRBE maps account for the Galactic FIRAS spectra with no need for a very cold component ({\$}T{\~{}}7{\$} K).},
author = {Lagache, G and Abergel, A and Boulanger, F and Puget, J L},
title = {{The interstellar cold dust observed by COBE}},
year = {1998}
}
